{
    "name": "thelhc/custom-auth",
    "description": "Custom auth provider and auth resources for laravel apps",
    "authors": [
        {
            "name": "Aaron Michael Music",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/auth": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "orchestra/testbench": "~3.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "TheLHC\\CustomAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheLHC\\CustomAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
